      <Query>
        <DataSourceName>NMMST_HR</DataSourceName>
        <CommandText>SELECT hmd201.* , hmd101.* , ( SELECT IsNull(SUM(hme101c_onduty),0) FROM hme101c WHERE hme101c_vid = hmd201_vid ) as onduty  , 
dbo.uf_yyyy_to_yyy_format( hmd201_bday , 'A6' , 'A' ) as bday , dbo.uf_yyyy_to_yyy_format( GetDate() , 'B6' , 'A' ) as nowdate
FROM hmd201 INNER JOIN hmd101 ON hmd201_tid = hmd101_tid
WHERE hmd201_vid = @vid</CommandText>
        <QueryParameters>
          <QueryParameter Name="@vid">
            <Value>=Parameters!vid.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
