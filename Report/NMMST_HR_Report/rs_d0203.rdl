      <Query>
        <DataSourceName>NMMST_HR</DataSourceName>
        <CommandText>Select
hmd201_cname,
(CASE WHEN hmd201_gent = 'M' THEN '男'  WHEN hmd201_gent = 'F' THEN '女' ELSE '' END) as hmd201_gent,
 dbo.uf_yyyy_to_yyy_format( hmd201_bday , 'A6' , 'A' ) as bday,
hmd201_ssn,
(SELECT hca203_name FROM hca203 WHERE hca203_id = hmd201_eduid) as hmd201_eduid,
(SELECT hca204_name FROM hca204 WHERE hca204_id = hmd201_jobid) as hmd201_jobid,
hmd201_joborg,
hmd201_jobtitle,
hmd201_addid,
hmd201_addot,
hmd201_email,
hmd201_phnow + (CASE WHEN IsNull(hmd201_phnowex,'') &lt;&gt; '' THEN '#' + IsNull(hmd201_phnowex,'') ELSE '' END) as hmd201_tel,
hmd201_phnoa,
hmd201_phnom,
hmd201_introtext,
hmd201_ps,
hmd201_notel,
hmd201_expert,
hmd201_weburl
From hmd201_t</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
