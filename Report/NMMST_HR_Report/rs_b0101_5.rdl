      </Fields>
      <Query>
        <DataSourceName>NMMST_HR</DataSourceName>
        <CommandText>select (ROW_NUMBER() OVER (ORDER BY hmc101_id) - 1) / 2 + 1 AS TitleRow,
       (ROW_NUMBER() OVER (ORDER BY hmc101_id) - 1) % 2 + 1 AS TitleColumn,
        RTrim(IsNull(hmc101_addid,'')) as hmc101_addid,
        RTrim(IsNull(hmc101_addot,'')) as hmc101_addot,
        hmc101_joborg,
        hmc101_jobdepartment,
        hmc101_id ,
        hmc101_cname ,
        (Case hmc101_gent WHEN 'M' THEN '先生' WHEN 'F' THEN '小姐' ELSE '' END),
        hmc101_jobtitle
From hmc101
Where hmc101_id in (select * from dbo.uf_split(@as_id,'|'))</CommandText>
        <QueryParameters>
          <QueryParameter Name="@as_id">
            <Value>=Parameters!as_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
