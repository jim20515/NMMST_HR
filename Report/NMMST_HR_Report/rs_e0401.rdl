        </Field>
        <Field Name="foodCT">
          <DataField>foodCT</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="foodMoney">
          <DataField>foodMoney</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>NMMST_HR</DataSourceName>
        <CommandText>Select ( SELECT hmd201_bookid From hmd201 Where hmd201_vid = hme101c_vid ) as vid , ( SELECT hmd201_cname From hmd201 Where hmd201_vid = hme101c_vid ) as vname , 
SUM( hme101b_foodCT ) as foodCT , SUM( hme101b_foodCT ) * 60 as foodMoney From hme101b , 
hme101c 
Where hme101b_pdid = hme101c_pdid  
AND Convert(char(6),hme101b_sdate,112)  &gt;=  @Syear + @Smonth
AND Convert(char(6),hme101b_sdate,112)  &lt;= @Eyear + @Emonth
AND hme101c_onduty &gt; 0 AND hme101b_foodCT &gt; 0 GROUP BY hme101c_vid</CommandText>
        <QueryParameters>
          <QueryParameter Name="@Syear">
            <Value>=Parameters!Syear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Smonth">
            <Value>=Parameters!Smonth.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Eyear">
            <Value>=Parameters!Eyear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Emonth">
            <Value>=Parameters!Emonth.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
