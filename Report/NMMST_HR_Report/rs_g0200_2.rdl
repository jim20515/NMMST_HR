      <Query>
        <DataSourceName>NMMST_HR</DataSourceName>
        <CommandText>select *
From hmd201 Inner Join hmd101 ON hmd201_tid = hmd101_tid Inner Join hle501 ON hle501_syear =  @as_year  AND hle501_vid = hmd201_vid AND hle501_tid = hmd201_tid 
WHERE hle501_score4 &gt; @as_score</CommandText>
        <QueryParameters>
          <QueryParameter Name="@as_year">
            <Value>=Parameters!as_year.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@as_score">
            <Value>=Parameters!as_score.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
