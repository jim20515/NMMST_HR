      <Query>
        <DataSourceName>NMMST_HR</DataSourceName>
        <CommandText>select *, dbo.uf_yyyy_to_yyy_format(getdate(),'B6','A')
From hmd201 Inner Join hmd101 ON hmd201_tid = hmd101_tid 
Where dbo.uf_getvyearplus(hmd201_vid) &gt;= 3
AND(SELECT IsNUll(SUM(onduty),0) FROM hme101bc WHERE vid = hmd201_vid AND NOT(Onduty is null) AND SDT &gt; IsNull(hmd201_lappdt,'') ) &gt;= 300</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
